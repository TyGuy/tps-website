#!/bin/bash

echo Loading Data. This will probably take about 2 minutes.
echo ...
echo ...

echo Importing past/current mentee data...
rake import_mentees

echo Importing current mentor data...
rake import_mentors

echo Importing Berkeley Mentors from google drive...
rake import_berkeley_mentors

echo Importing core team members...
rake import_core_members

echo Importing cohorts...
rake import_cohorts

echo Importing mentee applications from google drive...
rake import_mentee_apps

echo Importing mentor applications from google drive...
rake import_mentor_apps

echo Data load finished!